<h1>Characters#show</h1>
</br>
<ul class="list-inline">
  <li class="list-inline-item">Character: <%= @character.name %></li>
  <li class="list-inline-item">Age Of Character: <%= @character.age %></li>
  <li class="list-inline-item">Status:<%= @character.status %></li>
  <li class="list-inline-item">Owner: <%= @character.user_id %></li>
  <li class="list-inline-item"><%= link_to 'Delete', character_path(@character), method: :delete, data: {confirm: "Are you sure"} %></li>
  </br>
</ul>